{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "noImplicitAny": false,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "declaration": false,
    "emitDeclarationOnly": false,
    "sourceMap": false,
    "outDir": "./js-template",
    "rootDir": "./ts-template",
  },
  "include": ["ts-template/**/*.ts"]
}